@prefix : <https://www.ritualgrammar.org/ontology/> .
@prefix rg: <https://www.ritualgrammar.org/ontology/> .
@prefix np: <https://www.ritualgrammar.org/ontology/nepal/> .
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

@base <https://www.ritualgrammar.org/ontology/> .

#################################################################
# Ontology
#################################################################

<https://www.ritualgrammar.org/ontology/> rdf:type owl:Ontology ;
    owl:imports crm: ;
    owl:versionInfo "beta-1.0" .


#################################################################
#    Annotation Property defining metadata of curation
#################################################################	

dc:title rdf:type owl:AnnotationProperty .
dc:creator rdf:type owl:AnnotationProperty .
dcterms:description rdf:type owl:AnnotationProperty .
dcterms:date rdf:type owl:AnnotationProperty .	

#################################################################
#    Object Properties: Broader
#################################################################

### http://www.w3.org/2004/02/skos/core#broaderTransitive
skos:broaderTransitive rdf:type owl:ObjectProperty ,
                                owl:TransitiveProperty ;
    rdfs:label "has broader transitive"@en ;
    skos:definition "skos:broaderTransitive is a transitive superproperty of skos:broader."@en ;
    rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .

### http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf skos:broaderTransitive ;
    owl:inverseOf skos:narrower ;
    rdfs:label "has broader"@en ;
    skos:definition "skos:broader is used to state a direct hierarchical link between two concepts."@en ;
    rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .

#################################################################
#    Object Properties: Narrower
#################################################################

### http://www.w3.org/2004/02/skos/core#narrowerTransitive
skos:narrowerTransitive rdf:type owl:ObjectProperty ,
                                 owl:TransitiveProperty ;
    owl:inverseOf skos:broaderTransitive ;
    rdfs:label "has narrower transitive"@en ;
    skos:definition "skos:narrowerTransitive is a transitive superproperty of skos:narrower."@en ;
    rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .

### http://www.w3.org/2004/02/skos/core#narrower
skos:narrower rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf skos:narrowerTransitive ;
    rdfs:label "has narrower"@en ;
    skos:definition "skos:narrower is used to state a direct hierarchical link between two concepts."@en ;
    rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .    

#################################################################
#    Object Properties: inScheme
#################################################################    

### http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:ObjectProperty ;
    rdfs:label "is in scheme"@en ;
    rdfs:range skos:ConceptScheme .

#################################################################
#    Object Properties: hasTopConcept/topConceptOf
#################################################################    

### http://www.w3.org/2004/02/skos/core#hasTopConcept
skos:hasTopConcept rdf:type owl:ObjectProperty ;
    owl:inverseOf skos:topConceptOf ;
    rdfs:domain skos:ConceptScheme ;
    rdfs:range skos:Concept .

### http://www.w3.org/2004/02/skos/core#topConceptOf
skos:topConceptOf rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf skos:inScheme ;
    owl:inverseOf skos:hasTopConcept ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:ConceptScheme .

#################################################################
#    Object Properties: Related
#################################################################   

skos:related
    a owl:ObjectProperty ;
    rdfs:label "has related"@en ;
    rdfs:comment "Relates a concept to a concept with which there is an associative semantic relationship."@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept ;
    rdf:type owl:SymmetricProperty .


#################################################################
# Annotation properties 
#################################################################

skos:altLabel rdf:type owl:AnnotationProperty .
skos:definition rdf:type owl:AnnotationProperty .
skos:prefLabel rdf:type owl:AnnotationProperty .

#################################################################
# Concept Scheme (Vocabulary Backbone)
#################################################################

rg:RitualGrammarScheme
    rdf:type skos:ConceptScheme ;
    skos:prefLabel "Ritual Grammar Controlled Vocabulary"@en ;
    skos:definition "A SKOS-based controlled vocabulary aligned with CIDOC-CRM for modeling ritual types and classifications."@en ;
    skos:hasTopConcept rg:Ritual .

#################################################################
# Core Concepts (Rituals)
#################################################################

rg:Ritual
    rdf:type crm:E55_Type , skos:Concept ;
    skos:prefLabel "Ritual"@en ;
    skos:definition "A formalized sequence of actions performed according to cultural or religious tradition."@en ;
    skos:topConceptOf rg:RitualGrammarScheme ;
    skos:inScheme rg:RitualGrammarScheme .
	
### Religious ritual is a sub type of ritual   
rg:ReligiousRitual
    rdf:type crm:E55_Type , skos:Concept ;
    skos:broader rg:Ritual ;
    skos:prefLabel "Religious ritual"@en ;
    skos:definition "Rituals based on religions."@en ;
    skos:inScheme rg:RitualGrammarScheme .	
	
### Local ritual is a sub type of ritual   
rg:LocalRitual
    rdf:type crm:E55_Type , skos:Concept ;
    skos:broader rg:Ritual ;
    skos:prefLabel "Local/Folk ritual"@en ;
    skos:definition "Rituals based on local practices and cultures."@en ;
    skos:inScheme rg:RitualGrammarScheme .	

#################################################################
# Core Concepts (Religious Rituals)
#################################################################

### Hindu ritual is a sub type of religious ritual   
rg:HinduRitual
    rdf:type crm:E55_Type , skos:Concept ;
    skos:broader rg:ReligiousRitual ;
    skos:prefLabel "Hindu ritual"@en ;
    skos:inScheme rg:RitualGrammarScheme .

### Smarta ritual is a sub type of hindu ritual   
rg:SmartaRitual
    rdf:type crm:E55_Type , skos:Concept ;
    crm:P67i_is_referred_to_by np:HistoryOfIndianLiterature ;
    skos:broader rg:HinduRitual ;
    skos:prefLabel "Smarta ritual"@en ;
    skos:definition "Rituals based on Smriti texts, following the orthodox Vedic-Puranic synthesis."@en ;
    skos:inScheme rg:RitualGrammarScheme .

### Buddhist ritual is a sub type of religious ritual   
rg:BuddhistRitual
    rdf:type crm:E55_Type , skos:Concept ;
    skos:broader rg:ReligiousRitual ;
    skos:prefLabel "Buddhist ritual"@en ;
    skos:inScheme rg:RitualGrammarScheme .

#################################################################
# Core Concepts (Marriage Rituals)
#################################################################

rg:MarriageRitual
    rdf:type crm:E55_Type , skos:Concept ;
    skos:broader rg:Ritual ;
    skos:prefLabel "Marriage ritual"@en ;
    skos:inScheme rg:RitualGrammarScheme .
	
