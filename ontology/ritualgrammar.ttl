#################################################################
# Activities
#################################################################

rg:ChantingHinduMantra
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:Priest ;
    crm:P2_has_type rg:HinduMantraRole ;
    rdfs:label "Chanting Mantras: Hindu"@en .

rg:ChantingBuddhistMantra
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:Gubaju ;
    crm:P2_has_type rg:BuddhistMantraRole ;
    rdfs:label "Chanting Mantras: Buddhist"@en .

rg:SagunActivity
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:Priest ;
    crm:P16_used_specific_object rg:Egg ;
    rdfs:label "Auspicious Egg Offering"@en .

rg:OfferingToDeity
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:Bride , rg:Groom ;
    crm:P120_occurs_before rg:ChantingHinduMantra ;
    rdfs:label "Offering to deity"@en .

### Activities specific to Smarta/Parbatiya Tradition
rg:VermilionApplication
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:Groom ;
    crm:P2_has_type rg:SmartaRitual ;
    crm:P16_used_specific_object rg:Vermilion ;
    rdfs:label "Application of vermilion"@en .

rg:SaptapadiActivity
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:Bride , rg:Groom ;
    crm:P2_has_type rg:SmartaRitual ;
    rdfs:label "Saptapadi (Seven Steps)"@en .

rg:KanyadanaRitualIntensionActivity
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:HinduParent, rg:Priest ;
	crm:P11_had_participant rg:HinduBride , rg:HinduGroom  ;
    crm:P2_has_type rg:SmartaRitual ;
    rdfs:label "Kanyadan: Ritual Intension"@en .    
	
rg:KanyadanaPraisingGroomActivity
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:HinduParent ;
	crm:P11_had_participant rg:HinduBride , rg:HinduGroom, rg:Priest  ;
    crm:P2_has_type rg:SmartaRitual ;
    rdfs:label "Kanyadan: Praising Groom"@en .	

rg:KanyadanaBrideAsGiftActivity
    rdf:type crm:E7_Activity ;
    crm:P14_carried_out_by rg:HinduParent ;
	crm:P11_had_participant rg:HinduBride , rg:HinduGroom, rg:Priest  ;
    crm:P2_has_type rg:SmartaRitual ;
    rdfs:label "Kanyadan: Bride as Gift"@en .	


#################################################################
# Events
#################################################################

### Generic Event
rg:SagunEvent
    rdf:type crm:E5_Event ;
    crm:P11_had_participant rg:Bride , rg:Groom , rg:Parents ;
    crm:P2_has_type rg:SagunRitual ;
    crm:P9_consists_of rg:SagunActivity ;
    rdfs:label "Sagun offering ceremony"@en .

### Generic Marriage Event
rg:SmartaKanyadanaEvent
    rdf:type crm:E5_Event ;
    crm:P9_consists_of rg:KanyadanaRitualIntensionActivity,
					   rg:KanyadanaPraisingGroomActivity,
					   rg:KanyadanaBrideAsGiftActivity,
					   rg:ChantingHinduMantra ;
    rdfs:label "Smarta Kanyadana"@en .	
#################################################################
# Marriage Events based on different ethnic and caste systems
#################################################################

### Parbatiya Brahmin Marriage Event
np:ParbatiyaMarriageEssentialEvent
    rdf:type crm:E5_Event ;
    crm:P11_had_participant np:ParbatiyaBrahminCommunity , rg:Priest ;
    crm:P2_has_type np:ParbatiyaMarriageRitual ;
    crm:P9_consists_of np:ParbatiyaKanyadanaEvent ;
    skos:altLabel "Essential Events in Parbatiya Brahmin Marriage Ceremony" ;
    rdfs:label "Parbatiya Brahmin Marriage Event"@en .
	
np:ParbatiyaKanyadanaEvent
    rdf:type crm:E5_Event ;
    crm:P9_consists_of rg:SmartaKanyadanaEvent ;
    skos:altLabel "Kanyadana events at Parbatiya Brahmin Marriage Ceremony"@en ;
    rdfs:label "Kanyadana at Parbatiya Brahmin Marriage Event"@en .	



np:NewarHinduSwayambharaEvent
    rdf:type crm:E5_Event ;
    crm:P11_had_participant np:NewarCommunity ;
    crm:P2_has_type np:NewarSwayambharaRitual ;
    crm:P9_consists_of rg:ChantingHinduMantra ,
                       rg:OfferingToDeity ,
                       rg:VermilionApplication ;
    rdfs:label "Newar Swayambhara ceremony"@en .

np:NewarHinduMarriageEvent
    rdf:type crm:E5_Event ;
    crm:P11_had_participant np:NewarCommunity ;
    crm:P2_has_type np:NewarHinduMarriageRitual ;
    crm:P10i_contains rg:SagunEvent ,
                      np:NewarHinduSwayambharaEvent ;
    rdfs:label "Newar Hindu marriage"@en .

np:NewarBuddhistMarriageEvent 
    rdf:type crm:E5_Event ;
    crm:P11_had_participant np:NewarCommunity ;
